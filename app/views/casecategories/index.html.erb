<p id="notice"><%= notice %></p>

<h1>Casecategories</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Caption</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @casecategories.each do |casecategory| %>
      <tr>
        <td><%= casecategory.tag %></td>
        <td><%= casecategory.caption %></td>
        <td><%= link_to 'Show', casecategory %></td>
        <td><%= link_to 'Edit', edit_casecategory_path(casecategory) %></td>
        <td><%= link_to 'Destroy', casecategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Casecategory', new_casecategory_path %>
