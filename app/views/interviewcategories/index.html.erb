<p id="notice"><%= notice %></p>

<h1>Interviewcategories</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Caption</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interviewcategories.each do |interviewcategory| %>
      <tr>
        <td><%= interviewcategory.tag %></td>
        <td><%= interviewcategory.caption %></td>
        <td><%= link_to 'Show', interviewcategory %></td>
        <td><%= link_to 'Edit', edit_interviewcategory_path(interviewcategory) %></td>
        <td><%= link_to 'Destroy', interviewcategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interviewcategory', new_interviewcategory_path %>
